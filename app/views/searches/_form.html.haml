- if @search.errors.present?
  = @search.errors.full_messages.join("<br />").html_safe
= form_for @search, :html => { :class => 'form-horizontal form-model' } do |f|
  .form-group
    = f.label :query, :class => 'control-label col-md-2'
    .col-md-10
      = f.text_field :query, :class => 'text-field form-control', autocomplete: "off"

